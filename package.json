{
  "name": "higlass-bigwig-datafetcher",
  "version": "0.1.3",
  "description": "Bigwig Data Fetcher for HiGlass",
  "private": false,
  "author": "Alexander Veit",
  "license": "MIT",
  "contributors": [
    {
      "name": "Alexander Veit",
      "url": "http://github.com/alexander-veit"
    }
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/higlass/higlass-bigwig-datafetcher.git"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "cross-env": "^7.0.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "esbuild": "^0.14.42"
  },
  "scripts": {
    "build": "rimraf ./dist/ && npm run build-lib",
    "build-lib": "pnpm run build-lib:prod && pnpm run build-lib:dev",
    "build-lib:prod": "cross-env APP_ENV=production node ./bundle.mjs",
    "build-lib:dev": "cross-env APP_ENV=development node ./bundle.mjs",
    "start": "webpack-dev-server --mode development --open",
    "prerelease": "rm -rf dist/*; npm run build; zip -r dist.zip dist",
    "test-watch": "NODE_ENV=development; npm run build-debug && ./node_modules/karma/bin/karma start karma.conf.js",
    "test": "karma start karma.conf.js --single-run"
  },
  "dependencies": {
    "@gmod/bbi": "^3.0.1",
    "generic-filehandle": "^3.0.1",
    "d3-scale": "^1.0.7",
    "d3-dsv": "^3.0.1",
    "d3-request": "^1.0.6",
    "higlass-register": "^0.3.0",
    "slugid": "^3.0.0"
  }
}
