  "repository": {
    "type": "git",
    "url": "git://github.com/higlass/higlass-bigwig-datafetcher.git"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "autoprefixer": "^8.6.5",
    "babel-loader": "^8.2.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "jasmine": "^2.99.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.20.3",
    "sourcemap": "^0.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "unminified-webpack-plugin": "^2.0.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.11.3",
    "webpack": "^4.20.2"
  },
  "scripts": {
    "build-debug": "NODE_ENV=development webpack",
    "build-es": "rm -rf ./es/* && npx babel ./src/ --out-dir ./es/ --env-name esm",
    "build": "npm run build-es && NODE_ENV=production webpack --mode production",
    "start": "webpack-dev-server --mode development --open",
    "prerelease": "rm -rf dist/*; npm run build; zip -r dist.zip dist",
    "test-watch": "NODE_ENV=development; npm run build-debug && ./node_modules/karma/bin/karma start karma.conf.js",
    "test": "karma start karma.conf.js --single-run"
  },
  "dependencies": {
    "@gmod/bbi": "1.0.30",
    "generic-filehandle": "^2.0.2",
    "@babel/cli": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "d3-scale": "^1.0.7",
    "d3-dsv": "^3.0.1",
    "d3-request": "^1.0.6",
    "higlass-register": "^0.3.0",
    "slugid": "^3.0.0"
  }
